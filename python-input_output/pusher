#!/bin/bash

chmod -R 755 .
clear
read -p "What would you like me to post master? :: " message
git add .
git commit -m "$message"
git push

if [ $? -eq 0 ]; then
    clear
    echo "That worked master! Too bad that you suck at coding!"
else
    echo "That didn't work stupid! see you suck at everything!"
fi